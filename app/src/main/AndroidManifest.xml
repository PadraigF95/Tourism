<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ie.wit.tourism">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:name="ie.wit.tourism.main.NewRossTourism">
        <activity android:name=".activities.Home"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.Edit"></activity>
        <activity android:name=".activities.Search" />
        <activity android:name=".activities.Help" />
        <activity android:name=".activities.Add" />
        <activity android:name=".activities.Login" />
        <activity android:name=".activities.Register" />
        <activity android:name=".activities.Splashscreen" />
        <activity android:name=".activities.Logout" />
        <activity android:name=".activities.Currency_Converter" />
    </application>

</manifest>